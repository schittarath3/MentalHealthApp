<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<VBox alignment="CENTER" spacing="20.0"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.healthhgt8.mentalhealthapp.LandingController"
        styleClass="bg-default">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Sign in" styleClass="h2"/>
    <HBox alignment="CENTER" spacing="10">
        <Button fx:id="seekerSignInBtn" text="Help-Seeking" onAction="#startSeekerScreen"
                styleClass="btn, btn-primary"/>
        <Button fx:id="volunteerSignInBtn" text="Volunteer" onAction="#startVolunteerScreen"
                styleClass="btn, btn-primary"/>
    </HBox>
</VBox>
