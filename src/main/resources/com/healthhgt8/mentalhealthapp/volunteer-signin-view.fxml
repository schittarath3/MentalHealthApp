<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.CheckBox?>

<?import javafx.scene.control.Button?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.healthhgt8.mentalhealthapp.VolunteerSignInController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Label text="Enter name:"/>
    <TextField fx:id="nameInput"/>
    <Label fx:id="validName"/>

    <Label text="Enter School:"/>
    <TextField fx:id="schoolInput"/>
    <Label fx:id="validSchool"/>

    <Label text="Enter Major:"/>
    <TextField fx:id="majorInput"/>
    <Label fx:id="validMajor"/>

    <Label text="Enter Year:"/>
    <TextField fx:id="yearInput"/>
    <Label fx:id="validYear"/>

    <Button fx:id="SignIn" text="Sign Up" onAction="#startVolunteerScreen"/>
</VBox>
